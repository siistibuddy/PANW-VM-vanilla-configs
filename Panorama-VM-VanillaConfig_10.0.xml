<?xml version="1.0"?>
<config version="10.0.0" urldb="paloaltonetworks">
  <mgt-config>
    <devices/>
    <users>
      <entry name="admin">
        <phash>$1$gkehcdmb$xNve7m4autNAKeZlpwYk//</phash>
        <permissions>
          <role-based>
            <superuser>yes</superuser>
          </role-based>
        </permissions>
      </entry>
    </users>
    <password-complexity>
      <enabled>yes</enabled>
      <minimum-length>8</minimum-length>
    </password-complexity>
  </mgt-config>
  <shared/>
  <devices>
    <entry name="localhost.localdomain">
      <deviceconfig>
        <system>
          <ip-address>192.168.1.1</ip-address>
          <netmask>255.255.255.0</netmask>
          <update-server>updates.paloaltonetworks.com</update-server>
          <update-schedule>
            <threats>
              <recurring>
                <weekly>
                  <day-of-week>wednesday</day-of-week>
